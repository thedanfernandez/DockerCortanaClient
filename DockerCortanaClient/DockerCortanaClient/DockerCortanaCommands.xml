<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.1">
	<CommandSet xml:lang="en-us" Name="DockerCortanaCommandSet_en-us">
    <CommandPrefix> Docker, </CommandPrefix>
		<Example> run moby </Example>

		<Command Name="runContainer">
			<Example> run moby</Example>
			<ListenFor> run {container} </ListenFor>
			<Feedback> running container {container} </Feedback>
			<Navigate />
		</Command>
    
		<PhraseList Label="container">
			<Item> moby </Item>
		</PhraseList>
	</CommandSet>
  
</VoiceCommands>
